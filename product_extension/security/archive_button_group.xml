<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Product Archive access group -->
    <record id="group_product_archive_access" model="res.groups">
        <field name="name">Product Archive</field>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
    <record model="res.groups" id="group_archive">
        <field name="name">Hide Archive</field>
    </record>
    <record model="res.groups" id="group_set_quantity_zero">
        <field name="name">Set Quantity Zero</field>
    </record>
</odoo>
